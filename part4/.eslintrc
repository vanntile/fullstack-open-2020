{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": ["jest"],
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended",
    "airbnb",
    "plugin:jest/recommended",
    "plugin:jest/style"
  ],
  "rules": {
    "no-console": 0,
    "semi": "off"
  }
}
